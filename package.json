{
  "name": "ember-app-explorer",
  "version": "0.16.0",
  "description": "Ember.js app explorer",
  "main": "index.js",
  "bin": {
    "ember-app-explorer": "bin/cli.js",
    "eax": "bin/cli.js"
  },
  "scripts": {
    "test": "qunit tests/*-test.js",
    "deploy": "git push && git push --tags && npm publish",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint ."
  },
  "keywords": [
    "cli",
    "ember",
    "ember.js",
    "terminal",
    "explorer"
  ],
  "author": "Rajasegar Chandran <rajasegar.c@gmail.com>",
  "repository": "https://github.com/rajasegar/eax",
  "license": "MIT",
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.20",
    "cli-highlight": "^2.1.4",
    "compression-stats-cli": "^1.2.2",
    "filesize": "^6.1.0",
    "ramda": "^0.27.0",
    "semver": "^7.3.2",
    "voca": "^1.4.0",
    "walk-sync": "^2.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "release(version): Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">= 10.16.0"
  }
}
